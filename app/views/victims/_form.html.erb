<%= form_for(@victim) do |f| %>
  <% if @victim.errors.any? %>
    <div id="error_explanation">
      <% @victim.errors.full_messages.each do |msg| %>
        	<div class="alert alert-error">
 				<%= msg %>
			</div><br />
      <% end %>
    </div>
  <% end %>
  <%= f.hidden_field :user_id, :value => connected_user.id %>
  <div class="field">
    <%= f.label :fullname, label='Adı Soyadı: ',:class=>'label label-warning' %><br />
    <%= f.text_field :fullname %>
  </div>
  <div class="field">
    <%= f.label :twitter_username , label='Twitter Kullanıcı Adı: ', :class=>'label label-warning' %><br />
    <%= f.text_field :twitter_username %>
  </div>
  <div class="field">
    <%= f.label :facebook_username, label='Facebook Kullanıcı Adı: ',:class=>'label label-warning' %><br />
    <%= f.text_field :facebook_username %>
  </div>
  <div class="actions">
    <%= f.submit 'Kaydet', :class=>'btn btn-success' %>
  </div>
<% end %>
